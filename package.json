{
  "name": "@averay/gulp-clean-unmodified",
  "author": "Adam Averay (https://adamaveray.com.au/)",
  "description": "A Gulp plugin to remove unmodified files after tasks have completed.",
  "version": "1.0.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "src/index.ts"
  },
  "engines": {
    "bun": ">=1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamaveray/gulp-clean-unmodified.git"
  },
  "scripts": {
    "format": "bun x codeformat fix",
    "lint": "bun x codeformat check",
    "release": "bun --bun x bumpp && bun publish --access public",
    "prepare": "bun --bun x husky"
  },
  "peerDependencies": {
    "gulplog": "^2.0.1"
  },
  "devDependencies": {
    "@averay/codeformat": "^0.2.11",
    "@types/bun": "^1.2.19",
    "@types/vinyl": "^2.0.12",
    "bumpp": "^10.2.1",
    "husky": "^9.1.7",
    "typescript": "^5.8.3",
    "vinyl": "^3.0.1"
  }
}
